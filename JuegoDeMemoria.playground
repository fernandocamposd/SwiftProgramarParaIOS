//: Playground - noun: a place where people can play

import UIKit

var numeros = 0...100

for i in numeros {

    if i != 0 && i % 5 == 0 {
        print("\(i)\tBingo!!!")
    }
    if i != 0 && i % 2 == 0 {
        print("\(i)\tpar!!!")
    }
    if i != 0 && i % 2 != 0 {
        print("\(i)\timpar!!!")
    }
    
    if i > 30 && i < 40 {
        print("\(i)\tViva Swift!!!")
    }

}
